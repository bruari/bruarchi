#!/bin/bash

# Link all the key config files to bruarchi's internal versions for quicker testing
ln -nsf ~/.local/share/bruarchi/config/hypr/hyprland.conf ~/.config/hypr/
ln -nsf ~/.local/share/bruarchi/config/hypr/hypridle.conf ~/.config/hypr/
ln -nsf ~/.local/share/bruarchi/config/waybar/config ~/.config/waybar/
ln -nsf ~/.local/share/bruarchi/config/wofi/config ~/.config/wofi/
